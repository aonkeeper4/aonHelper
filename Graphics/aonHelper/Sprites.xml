<?xml version="1.0" encoding="utf-8" ?>
<Sprites>
    <aonHelper_resizableHeart path="collectables/aonHelper/resizableHeart/" start="spin">
        <Center />

        <Loop id="idle" path="" frames="0" />
        <Loop id="spin" path="" frames="0*10,1-13" delay="0.1"/>
        <Loop id="fastspin" path="" delay="0.1"/>
    </aonHelper_resizableHeart>
    <aonHelper_resizableHeartOutline path="collectables/aonHelper/resizableHeart_Outline/" start="spin">
        <Center />

        <Loop id="idle" path="" frames="0" />
        <Loop id="spin" path="" frames="0*10,1-13" delay="0.1"/>
        <Loop id="fastspin" path="" delay="0.1"/>
    </aonHelper_resizableHeartOutline>

    <aonHelper_featherDashSwitch path="objects/aonHelper/featherDashSwitch/" start="idle">
        <Justify x="0.5" y="0.5"/>
        <Loop id="idle" path="" delay="0.08" frames="0-20"/>
        <Loop id="pushed" path="" delay="0.08" frames="27"/>
        <Anim id="push" path="" delay="0.07" frames="21-27" goto="pushed"/>
    </aonHelper_featherDashSwitch>

    <aonHelper_darkerMatterWarp path="objects/aonHelper/darkerMatter/" start="boost">
        <Loop id="boost" path="warp" delay="0.06" frames="0-21"/>
    </aonHelper_darkerMatterWarp>
</Sprites>